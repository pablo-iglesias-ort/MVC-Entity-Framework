@model IEnumerable<MVC_Entity_Framework.Models.Materia>

@{
    ViewData["Title"] = "Materias";
}

<h1>Materias</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Nombre)
                </td>
            </tr>
        }
    </tbody>
</table>
<div>
    <a asp-action="Details" asp-route-id=@ViewData["EstudianteId"] asp-route-otroCampo="hola">Volver al Estudiante</a>
</div>
